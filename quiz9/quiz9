#include <iostream> 
#include <iomanip> 
using namespace std;

struct Employee
{
int employeeID[6];
string firstName;
string lastName;
int Salary[6];
string department;
string month;
int day[2];
int year[4];
}

void findsalary
{
    ifstream myfile("employee.txt");
    myfile.open();
    Employee myArray[3];

        for(int i = 0; i < 3; ++i)
        {
            if (myArray[i].salary > 100000)
            cout << firstname << " " <<  lastname << "makes more than $100000"
        }
    myfile.close();
}

void finddept
{
    ifstream myfile("employee.txt");
    myfile.open();
    Employee myArray[3];

        for(int i = 0; i < 3; ++i)
        {
            if myArray[i].dept == "Computer"
            cout << firstname << " " <<  lastname << "is in the computer department";
        }
        myfile.close();
}


void readfile
{
 ifstream myfile("employee.txt");
    myfile.open();
    Employee myArray[3];

    {
        Employee myArray[3];

        for(int i = 0; i < 3; ++i)
        {
            file >> myArray[i];
        }
    }


   myfile.close();          

}

int main  ()
{
readfile();
findsalary();
finddept();
}