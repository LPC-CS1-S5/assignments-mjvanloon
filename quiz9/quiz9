struct employee
{
    #include <iostream> 
 #include <iomanip> 


    using namespace std;

    struct Employee
{

int employeeID[6]
string firstName;
string lastName;
int Salary[6];
string department;
string month;
int day[2];
int year[4];
};

}
void readFile(string filename, AccessRecord records[3])
{
   ifstream fin(filename);  
   if (fin.fail())          
      return 0;

   while (fin >> records[3]) 
   {                            
      int i = 0;                
      fin >> records[i]        
      i++;                      
   }                            

   fin.close();             

}
int main  ()
	ifstream ifso;
	ifso.open("employee.txt");
    readFile (ifso)